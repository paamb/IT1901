<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.HBox?>

<Pane fx:id="label" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.LabelController">
    <HBox maxHeight="20.0" maxWidth="135" minHeight="20.0" minWidth="135">
        <Button id="removeLabel" fx:id="removeLabel" maxHeight="20.0" minHeight="20.0" mnemonicParsing="false" onAction="#removeLabel" text="X" />
        <Label id="labelName" fx:id="labelName" prefHeight="20.0" prefWidth="115.0" text="Label" />
    </HBox>
</Pane>
